{
  "name": "transcendence-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "env-cmd -f ./.env/.env.prod next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start:dev": "docker compose -f docker-compose.dev.yaml -p transcendence up --build",
    "start:prod": "docker compose -f docker-compose.prod.yaml -p transcendence up --build",
    "start:test": "docker compose -f docker-compose.test.yaml -p transcendence up --build",
    "down": "docker compose -f docker-compose.dev.yaml -p transcendence down; docker compose -f docker-compose.prod.yaml -p transcendence down; docker compose -f docker-compose.test.yaml -p transcendence down",
    "test": "env-cmd -f ./.env/.env.test jest",
    "test:watch": "env-cmd -f ./.env/.env.test jest --watch",
    "test:cov": "env-cmd -f ./.env/.env.test jest --coverage"
  },
  "dependencies": {
    "@types/node": "^20.4.8",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "next": "13.4.13",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "env-cmd": "^10.1.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.6",
    "msw": "^1.2.3",
    "ts-jest": "^29.1.1"
  }
}
